{
  "name": "ms-sandbox",
  "version": "1.0.0",
  "description": "virtus ms sandbox",
  "main": "serverless.ts",
  "scripts": {
    "deploy": "npm run lint && sls deploy",
    "deploy:f": "npm run lint && sls deploy -f $npm_config_f",
    "deploy:prod": "./deploy.sh prod",
    "test": "jest",
    "lint": "npm run lint:format && npm run lint:duplicate && npm run lint:check",
    "lint:check": "npx tslint -t stylish --project tsconfig.json -c tslint.json 'src/**/*.ts'",
    "lint:duplicate": "npx jscpd ./src",
    "lint:fix": "npx tslint -t stylish --fix --project tsconfig.json -c tslint.json 'src/**/*.ts'",
    "lint:format": "npx prettier --write \"src/**/*.ts\" \"*.ts\" \"*.js\" \"*.json\" \"src/**/*.json\"",
    "tsc:interface": "npx utransfer json2ts"
  },
  "engines": {
    "node": ">= 18.0.0"
  },
  "keywords": [
    "nodeJs",
    "TS",
    "serverless"
  ],
  "license": "MIT",
  "devDependencies": {
    "@serverless/typescript": "3.30.1",
    "@types/joi": "17.2.3",
    "@types/node": "20.9.1",
    "copy-webpack-plugin": "11.0.0",
    "fork-ts-checker-webpack-plugin": "9.0.2",
    "serverless": "3.37.0",
    "serverless-domain-manager": "^7.3.3",
    "serverless-esbuild": "1.49.0",
    "serverless-offline": "13.3.0",
    "serverless-webpack": "5.13.0",
    "source-map-loader": "4.0.1",
    "terser-webpack-plugin": "5.3.9",
    "thread-loader": "4.0.2",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "tsconfig-paths-webpack-plugin": "4.1.0",
    "tslint": "6.1.3",
    "tslint-config-prettier": "1.18.0",
    "tsutils": "3.21.0",
    "typescript": "5.3.2",
    "utransfer-cli-demo": "^1.0.0",
    "utransfer-tsconfig": "1.0.2",
    "utransfer-tslint": "1.0.3",
    "webpack": "5.89.0",
    "webpack-cli": "5.1.4",
    "webpack-node-externals": "3.0.0"
  },
  "dependencies": {
    "@types/lodash": "4.14.202",
    "aws-lambda": "^1.0.7",
    "aws-sdk": "2.1498.0",
    "commander": "2.19.0",
    "inversify": "5.0.1",
    "jimp": "^0.3.5",
    "joi": "17.11.0",
    "json-schema-to-typescript": "13.1.1",
    "jsonpath": "1.1.1",
    "lodash": "4.17.21",
    "microtime": "3.1.1",
    "middy": "0.24.0",
    "npm": "10.2.4",
    "rcfile": "1.0.3",
    "reflect-metadata": "0.1.13",
    "rollbar": "2.26.2",
    "rxjs": "7.8.1",
    "rxjs-spy": "8.0.2",
    "utransfer-ms-core": "1.0.19"
  }
}
